<h1>New command_group</h1>

<% form_for(@command_group) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :Description %><br />
    <%= f.text_field :Description %>
  </p>
  <p>
    <%= f.label :Hint %><br />
    <%= f.text_field :Hint %>
  </p>
  <p>
    <%= f.label :CanTurnOff %><br />
    <%= f.text_field :CanTurnOff %>
  </p>
  <p>
    <%= f.label :AlwaysShow %><br />
    <%= f.text_field :AlwaysShow %>
  </p>
  <p>
    <%= f.label :CanBeHidden %><br />
    <%= f.text_field :CanBeHidden %>
  </p>
  <p>
    <%= f.label :AltID %><br />
    <%= f.text_field :AltID %>
  </p>
  <p>
    <%= f.label :AutoGeneratedDate %><br />
    <%= f.text_field :AutoGeneratedDate %>
  </p>
  <p>
    <%= f.label :Disabled %><br />
    <%= f.text_field :Disabled %>
  </p>
  <p>
    <%= f.label :TemplateParm1 %><br />
    <%= f.text_field :TemplateParm1 %>
  </p>
  <p>
    <%= f.label :TemplateParm2 %><br />
    <%= f.text_field :TemplateParm2 %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', command_groups_path %>
