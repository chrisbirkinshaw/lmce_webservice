<h1>Listing rooms</h1>

<table>
  <tr>
    <th>Installation</th>
    <th>RoomType</th>
    <th>Description</th>
    <th>Manuallyconfigureea</th>
    <th>Hidefromorbiter</th>
    <th>Floorplaninfo</th>
  </tr>

<% for room in @rooms %>
  <tr>
    <td><%= link_to room.installation.id, room.installation unless room.installation.nil? %></td>
    <td><%= link_to room.room_type.id, room.room_type unless room.room_type.nil? %></td>
    <td><%=h room.Description %></td>
    <td><%=h room.ManuallyConfigureEA %></td>
    <td><%=h room.HideFromOrbiter %></td>
    <td><%=h room.FloorplanInfo %></td>
    <td><%= link_to 'Show', room %></td>
    <td><%= link_to 'Edit', edit_room_path(room) %></td>
    <td><%= link_to 'Destroy', room, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New room', new_room_path %>
