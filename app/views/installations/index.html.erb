<h1>Listing installations</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Name</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Activationcode</th>
    <th>Laststatus</th>
    <th>Lastalive</th>
    <th>Isactive</th>
    <th>Ismonitored</th>
  </tr>

<% for installation in @installations %>
  <tr>
    <td><%=h installation.Description %></td>
    <td><%=h installation.Name %></td>
    <td><%=h installation.Address %></td>
    <td><%=h installation.City %></td>
    <td><%=h installation.State %></td>
    <td><%=h installation.Zip %></td>
    <td><%=h installation.ActivationCode %></td>
    <td><%=h installation.LastStatus %></td>
    <td><%=h installation.LastAlive %></td>
    <td><%=h installation.isActive %></td>
    <td><%=h installation.isMonitored %></td>
    <td><%= link_to 'Show', installation %></td>
    <td><%= link_to 'Edit', edit_installation_path(installation) %></td>
    <td><%= link_to 'Destroy', installation, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New installation', new_installation_path %>
