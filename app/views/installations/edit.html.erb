<h1>Editing installation</h1>

<% form_for(@installation) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :Description %><br />
    <%= f.text_field :Description %>
  </p>
  <p>
    <%= f.label :Name %><br />
    <%= f.text_field :Name %>
  </p>
  <p>
    <%= f.label :Address %><br />
    <%= f.text_field :Address %>
  </p>
  <p>
    <%= f.label :City %><br />
    <%= f.text_field :City %>
  </p>
  <p>
    <%= f.label :State %><br />
    <%= f.text_field :State %>
  </p>
  <p>
    <%= f.label :Zip %><br />
    <%= f.text_field :Zip %>
  </p>
  <p>
    <%= f.label :ActivationCode %><br />
    <%= f.text_field :ActivationCode %>
  </p>
  <p>
    <%= f.label :LastStatus %><br />
    <%= f.text_field :LastStatus %>
  </p>
  <p>
    <%= f.label :LastAlive %><br />
    <%= f.text_field :LastAlive %>
  </p>
  <p>
    <%= f.label :isActive %><br />
    <%= f.text_field :isActive %>
  </p>
  <p>
    <%= f.label :isMonitored %><br />
    <%= f.check_box :isMonitored %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @installation %> |
<%= link_to 'Back', installations_path %>
