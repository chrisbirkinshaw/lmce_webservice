<h1>Listing commands</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Define</th>
    <th>Avcommand</th>
    <th>Comments</th>
    <th>Log</th>
  </tr>

<% for command in @commands %>
  <tr>
    <td><%=h command.Description %></td>
    <td><%=h command.Define %></td>
    <td><%=h command.AVCommand %></td>
    <td><%=h command.Comments %></td>
    <td><%=h command.Log %></td>
    <td><%= link_to 'Show', command %></td>
    <td><%= link_to 'Edit', edit_command_path(command) %></td>
    <td><%= link_to 'Destroy', command, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New command', new_command_path %>
